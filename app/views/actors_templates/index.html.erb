<h1>List of all actors</h1>

<table border="1">
  <tr>
    <td>ID</td>
    <td>Image</td>
    <td>Name</td>
    <td>Dob</td>
    <td></td>
  </tr>
<% @list_of_actors.each do |actor| %>
<tr>
  <td><%= actor.id %></td>
  <td><img src="<%= actor.image %>"></td>
  <td><%= actor.name %></td>
  <td><%= actor.dob %></td>
  <td><a href="/actors/<%= actor.id %>">Show details</a></td>
</tr>
<%end%>
</table>
